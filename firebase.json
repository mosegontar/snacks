{
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/fetchPosts*",
        "function": "fetchPosts"
      },
      {
        "source": "**",
        "function": "renderIndex"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true,
    "trailingSlash": false
  }
}
